{
    "manifest_version": 3,
    "name": "Hall of iFrame · 页问",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "chrome_url_overrides": {
        "newtab": "custom_page.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "hall_of_iFrame@uxer"
        }
    }
}